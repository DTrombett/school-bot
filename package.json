{
  "name": "school-bot",
  "version": "1.0.0",
  "description": "A bot to organize school",
  "homepage": "https://github.com/DTrombett/school-bot#readme",
  "exports": "./dist/index.js",
  "scripts": {
    "build": "tsup",
    "commands": "node --experimental-specifier-resolution=node --enable-source-maps --trace-warnings dist/registerCommands.js",
    "lint": "eslint src --fix",
    "prepare": "npm test && husky install",
    "prettier": "prettier --write src/**/*.ts",
    "start:no-build": "node --experimental-specifier-resolution=node --enable-source-maps --trace-warnings ./dist/index.js",
    "start": "npm run build && npm run start:no-build",
    "test": "eslint src && tsc --noEmit"
  },
  "author": "DTrombett",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=16.9.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DTrombett/school-bot.git"
  },
  "bugs": {
    "url": "https://github.com/DTrombett/school-bot/issues"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@types/node": "^17.0.14",
    "@types/node-fetch": "^2.5.12",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^7.0.4",
    "tsup": "^5.11.13",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@discordjs/builders": "^0.12.0",
    "@discordjs/rest": "^0.3.0",
    "@sapphire/async-queue": "^1.2.0",
    "bufferutil": "^4.0.6",
    "cron-parser": "^4.2.1",
    "discord-api-types": "^0.26.1",
    "discord.js": "^14.0.0-dev.1644235768.b936103",
    "dotenv": "^16.0.0",
    "erlpack": "github:discord/erlpack",
    "playwright": "^1.18.1",
    "utf-8-validate": "^5.0.8",
    "zlib-sync": "^0.1.7"
  }
}
